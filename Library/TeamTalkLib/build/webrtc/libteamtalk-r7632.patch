Subject: [PATCH] Add teamtalk static library target to audio_processing
 BUILD.gn

---
 modules/audio_processing/BUILD.gn | 9 +++++++++
 1 file changed, 9 insertions(+)

diff --git a/modules/audio_processing/BUILD.gn b/modules/audio_processing/BUILD.gn
index 57af2d3d85..8acab8979f 100644
--- a/modules/audio_processing/BUILD.gn
+++ b/modules/audio_processing/BUILD.gn
@@ -230,6 +230,15 @@ rtc_library("audio_processing") {
   }
 }
 
+static_library("teamtalk") {
+  complete_static_lib = true
+  deps = [
+    ":audio_processing",
+    "../../api/audio:builtin_audio_processing_builder",
+    "../../api/environment:environment_factory",
+  ]
+}
+
 rtc_library("residual_echo_detector") {
   poisonous = [ "default_echo_detector" ]
   configs += [ ":apm_debug_dump" ]
